{
  "name": "isvalid",
  "version": "1.0.0",
  "description": "A simple functional library for creating custom validation rules",
  "main": "lib/isvalid.js",
  "scripts": {
    "bundle": "WEBPACK_ENV=bundle ./node_modules/webpack/bin/webpack.js",
    "compile": "WEBPACK_ENV=compile ./node_modules/webpack/bin/webpack.js",
    "test": "./node_modules/babel-tape-runner/bin/babel-tape-runner ./tests/**/*.js | ./node_modules/faucet/bin/cmd.js",
    "build": "rm -r ./lib/* &&  npm run bundle && npm run compile && ./node_modules/uglify-js/bin/uglifyjs ./lib/isvalid.js -c -m -o ./lib/isvalid.min.js --source-map ./lib/isvalid.min.js.map",
    "dev": "npm run bundle -- --progress --colors --watch"
  },
  "keywords": [
    "validate",
    "validation"
  ],
  "author": "Thomas Leenders",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-tape-runner": "^2.0.1",
    "eslint": "^2.12.0",
    "eslint-loader": "^1.3.0",
    "faucet": "0.0.1",
    "lodash": "^4.13.1",
    "tape": "^4.5.1",
    "uglify-js": "^2.6.2",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "lodash.assign": "^4.0.9",
    "lodash.mapvalues": "^4.4.0",
    "lodash.partial": "^4.1.4",
    "validator": "^5.4.0"
  }
}
